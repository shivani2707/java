package assignment_4;

public class Player {
	private int Health;
	 
	//create getHealth and setHealth methods
		//Think about parameters and return types
	
	public void setHealth(int h)
	{
		Health=h;
		
	}
public int getHealth()
{
	return Health;
	
	}
	
	public static void main(String[]args){
		Player shiv=new Player();
		shiv.setHealth(100);
		System.out.println(shiv.getHealth());
	}

	

	 
	

}
